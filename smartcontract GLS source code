// SPDX-License-Identifier: MIT
pragma solidity ^0.8.20;

contract GAULESCOIN {
    // Informações do token
    string public name = "GAULES COIN";
    string public symbol = "GLS";
    uint8 public decimals = 18;
    uint256 public totalSupply;

    // Percentuais das taxas (em base 10000, ex: 1% = 100, 3% = 300)
    uint256 public burnRate = 300; // 3%
    uint256 public liquidityRate = 100; // 1%
    uint256 public constant FEE_DENOMINATOR = 10000; // Base para percentuais

    // Endereços das carteiras
    address public deadAddress = 0x000000000000000000000000000000000000dEaD;
    address public liquidityAddress;

    // Mapeamento de saldos e transferências permitidas
    mapping(address => uint256) public balanceOf;
    mapping(address => mapping(address => uint256)) public allowance;

    // Proprietário do contrato
    address public owner;

    // Eventos
    event Transfer(address indexed from, address indexed to, uint256 value);
    event Approval(address indexed owner, address indexed spender, uint256 value);
    event OwnershipTransferred(address indexed previousOwner, address indexed newOwner);

    // Modificador para funções que apenas o proprietário pode executar
    modifier onlyOwner() {
        require(msg.sender == owner, "Apenas o proprietario pode executar esta acao");
        _;
    }

    // Construtor
    constructor(address _liquidityAddress) {
        require(_liquidityAddress != address(0), "Endereco de liquidez nao pode ser zero");
        liquidityAddress = _liquidityAddress;
        owner = msg.sender;
        totalSupply = 120000000000 * 10 ** uint256(decimals); // Suprimento total: 120 bilhões
        balanceOf[msg.sender] = totalSupply;
        emit Transfer(address(0), msg.sender, totalSupply);
    }

    // Transferência padrão
    function transfer(address recipient, uint256 amount) public returns (bool) {
        _transfer(msg.sender, recipient, amount);
        return true;
    }

    // Transferência que implementa taxas
    function transferWithFee(address recipient, uint256 amount) public returns (bool) {
        uint256 burnAmount = (amount * burnRate) / FEE_DENOMINATOR;
        uint256 liquidityAmount = (amount * liquidityRate) / FEE_DENOMINATOR;
        uint256 amountAfterFee = amount - burnAmount - liquidityAmount;

        // Transferir tokens para liquidez
        if (liquidityAmount > 0) {
            _transfer(msg.sender, liquidityAddress, liquidityAmount);
        }

        // Queimar tokens
        if (burnAmount > 0) {
            _transfer(msg.sender, deadAddress, burnAmount);
        }

        // Transferir o restante para o destinatário
        _transfer(msg.sender, recipient, amountAfterFee);
        return true;
    }

    // Transferência interna
    function _transfer(address from, address to, uint256 amount) internal {
        require(to != address(0), "Endereco do destinatario invalido");
        require(balanceOf[from] >= amount, "Saldo insuficiente");

        balanceOf[from] -= amount;
        balanceOf[to] += amount;
        emit Transfer(from, to, amount);
    }

    // Aprovação de gasto
    function approve(address spender, uint256 amount) public returns (bool) {
        allowance[msg.sender][spender] = amount;
        emit Approval(msg.sender, spender, amount);
        return true;
    }

    // Transferência a partir de uma aprovação
    function transferFrom(address sender, address recipient, uint256 amount) public returns (bool) {
        require(allowance[sender][msg.sender] >= amount, "Limite de aprovacao excedido");
        allowance[sender][msg.sender] -= amount;
        _transfer(sender, recipient, amount);
        return true;
    }

    // Função para alterar as taxas
    function setRates(uint256 _burnRate, uint256 _liquidityRate) external onlyOwner {
        require(_burnRate + _liquidityRate <= FEE_DENOMINATOR, "Total das taxas excede 100%");
        burnRate = _burnRate;
        liquidityRate = _liquidityRate;
    }

    // Transferir a propriedade do contrato
    function transferOwnership(address newOwner) public onlyOwner {
        require(newOwner != address(0), "Novo proprietario nao pode ser o endereco zero");
        emit OwnershipTransferred(owner, newOwner);
        owner = newOwner;
    }
}

